{
  "name": "@danielmoncada/angular-datetime-picker-dayjs-adapter",
  "version": "2.1.1",
  "description": "Angular Date Time Picker (DayJs Adapter)",
  "keywords": [
    "Angular",
    "datepicker",
    "date picker",
    "timepicker",
    "time picker",
    "datetime picker",
    "date time picker",
    "material",
    "ngx",
    "dayjs"
  ],
  "author": "Maintained and updated by Daniel Moncada, original implementatiom by Daniel Pan",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build picker",
    "test": "ng test picker",
    "test-with-coverage": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox --code-coverage=true picker",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build picker --configuration production && cp README.md dist/picker",
    "npm_pack": "cd dist/picker && npm pack",
    "package_windows": "npm run build_lib && npm run npm_pack",
    "package_linux": "npm run build_lib && npm run npm_pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielmoncada/date-time-picker-dayjs-adapter.git"
  },
  "dependencies": {
    "@angular/animations": "^12.2.17",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "core-js": "^3.33.3",
    "dayjs": "^1.11.10",
    "dayjs-plugin-utc": "^0.1.2",
    "np": "^6.2.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@danielmoncada/angular-datetime-picker": "^13.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.29",
    "codecov": "^3.6.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.7",
    "node-sass": "^9.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.6.2",
    "tslint": "~6.1.0",
    "typescript": "^4.3.5"
  }
}